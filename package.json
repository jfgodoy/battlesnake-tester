{
  "name": "battlesnake-tester",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint src && tsc  --noEmit --skipLibCheck  -p .",
    "update-styles": "curl -s https://play.battlesnake.com/references/customizations/ | grep -oP '(?<=<small>)[a-zA-Z-]+(?=</small>)' | sort | uniq | xargs -I {} bash -c 'curl -sf https://media.battlesnake.com/snakes/heads/{}.svg?board_version=1.0.38 -o src/assets/snakes/heads/{}.svg & curl -sf https://media.battlesnake.com/snakes/tails/{}.svg?board_version=1.0.38 -o src/assets/snakes/tails/{}.svg'"
  },
  "license": "MIT",
  "devDependencies": {
    "@iconify/json": "^1.1.396",
    "@tailwindcss/forms": "^0.3.3",
    "@types/ramda": "^0.27.44",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "autoprefixer": "^10.3.3",
    "eslint": "^7.32.0",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.9",
    "type-fest": "^2.1.0",
    "unplugin-auto-import": "^0.3.3",
    "unplugin-icons": "^0.7.5",
    "vite": "^2.5.3",
    "vite-plugin-solid": "^2.0.3",
    "vite-plugin-solid-svg": "^0.1.2"
  },
  "dependencies": {
    "idb": "^6.1.3",
    "nanoid": "^3.1.25",
    "ow": "^0.27.0",
    "ramda": "^0.27.1",
    "solid-js": "^1.1.2"
  }
}
